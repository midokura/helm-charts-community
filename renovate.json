{
  "extends": [
    "config:base"
  ],
  "dependencyDashboard": false,
  "timezone": "Europe/Madrid",
  "schedule": ["on monday"],
  "enabledManagers": ["helmv3", "helm-values"],
  "packageRules": [
    {
      "matchPackageNames": ["librenms/librenms", "nginx"],
      "versioning": "semver"
    },
    {
      "matchPackageNames": ["moul/sshportal"],
      "extractVersion": "^v(?<version>\\d+\\.\\d+\\.\\d+)$"
    }
  ]
}
